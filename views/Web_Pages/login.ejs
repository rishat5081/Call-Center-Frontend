<% include Web_Sections/header.ejs %>
<div class="container col-lg-6 col-xl-6 col-md-6 col-sm-6">
    <div class="align-content-center align-items-center">
        <div class="login_form_div">
            <% include Web_Sections/message.ejs %>
            <form action="/login" method="POST">
                <div class="form-group">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="email" class="form-control" name="user_loginemail" aria-describedby="emailHelp"
                        placeholder="Enter email" required>
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" class="form-control" name="user_login_password" placeholder="Password"
                        required>
                </div>
                <!-- <div class="form-check">
                    <input type="checkbox" class="form-check-input" id="exampleCheck1">
                    <label class="form-check-label" for="exampleCheck1">Check me out</label>
                </div> -->
                <div class="Login_Button">
                    <button type="submit" class="btn btn-dark">Login</button>
                </div>
            </form>
        </div>
    </div>
</div>
<% include Web_Sections/footer.ejs %>